    "version": "0.2.0",
    "configurations": [
    {
        "type": "browse-lite",
        "request": "attach",
        "name": "Browse Lite: Attach"
    },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/routes/postRoute.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ]
        }
    ]
}